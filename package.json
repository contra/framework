{
  "name": "kratos",
  "version": "1.4.0",
  "description": "Powerful full stack framework using the strength of the JavaScript ecosystem",
  "homepage": "https://github.com/kratos/kratos",
  "main": "dist/index.js",
  "files": ["dist"],
  "scripts": {
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test:coverage": "istanbul cover _mocha -- --reporter dot --check-leaks test/",
    "test:travis": "istanbul cover _mocha --report lcovonly -- --reporter spec --check-leaks test/",
    "clean": "rimraf dist",
    "prebuild": "npm run clean -s",
    "build": "babel --optional=runtime --whitelist=es6.modules,strict -d dist src",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "prepublish": "npm run build -s",
    "postpublish": "git push origin develop --follow-tags"
  },
  "repository": "kratos/kratos",
  "keywords": [
    "framework",
    "full",
    "stack",
    "koa",
    "api",
    "http",
    "web",
    "rest",
    "restful",
    "router",
    "app",
    "es6"
  ],
  "engines": {
    "iojs": ">= 1.0.0",
    "node": ">= 0.12.0"
  },
  "author": {
    "name": "Viarcore",
    "email": "support@viracore.com",
    "url": "https://www.viracore.com"
  },
  "contributors": [
    {
      "name": "Josh Manders",
      "email": "josh@joshmanders.com",
      "url": "https://certifiedweb.ninja"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "koa": "1.0.0",
    "koa-compress": "1.0.8",
    "koa-logger": "1.3.0",
    "koa-mount": "1.3.0",
    "koa-response-time": "1.0.2",
    "koa-views": "3.1.0",
    "kratos-config": "1.0.0",
    "kratos-router": "1.0.0",
    "lodash": "3.10.1",
    "process-env": "0.0.1"
  },
  "devDependencies": {
    "babel": "5.8.23",
    "babel-runtime": "5.8.24",
    "chai": "3.2.0",
    "istanbul": "0.3.20",
    "mocha": "2.3.2",
    "rimraf": "2.4.3",
    "standard": "5.2.2"
  }
}
